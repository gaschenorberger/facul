Algoritmo "Exercício 2"

Var
   b,c,valorHip: real
Inicio
       escreval("Entre com o valor de B")
       leia(b)
       
       escreval("Entre com o valor C")
       leia(c)
       
       valorHip <-0
       valorHip <- raizq(exp(b,2) + exp(c,2))
       
       escreva("O valor da Hipotenusa é:",valorHip)
Fimalgoritmo
