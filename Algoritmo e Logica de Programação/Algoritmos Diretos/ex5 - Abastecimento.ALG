Algoritmo "Exercício 5"

Var
   precGaso: real
   quantGaso: real
   vlTotal: real
   
Inicio
      precGaso <- 2.40
      vlTotal <- 0
      
      escreval("Abasteceu quantos litros?")
      leia(quantGaso)
      
      vlTotal <- quantGaso*precGaso
      
      escreva("Você irá pagar:",vlTotal)
      
Fimalgoritmo